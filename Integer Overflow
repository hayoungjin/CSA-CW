public class IntegerOverflow
{
    public static void main(String[] args)
    {
        int min = Integer.MIN_VALUE;
        int max = Integer.MAX_VALUE;
        System.out.println(min);
        System.out.println(max);
        min = min - 1;
        max = max + 1;
        System.out.println(min);
        System.out.println(max);
    }
}
